        <!-- Notes Section -->
        <div class="info-section">
            <h3>Notes</h3>
            <div class="notes-container">
                <textarea id="personalNotes" placeholder="Jot down your vibes..."></textarea>
                <p id="saveMessage">Saved!</p>
            </div>
            <button class="edit-button" onclick="toggleEditMode()">Edit</button>
            <button class="reset-button" onclick="resetSliders()">Reset Sliders</button>
        </div>

        <!-- Share Buttons Section -->
        <div class="share-container">
            <button class="share-button telegram" onclick="shareToTelegram()">Telegram</button>
            <button class="share-button whatsapp" onclick="shareToWhatsApp()">WhatsApp</button>
            <button class="share-button email" onclick="shareToEmail()">Email</button>
        </div>
    </div>

    <script>
        function updatePreference(slider, preferenceLabel) {
            const levels = ["Hard Limit", "Curious", "Moderate", "Oh Yes"];
            const colors = ["#FF4D4D", "#FF914D", "#FFD700", "#00CC66"];
            slider.value = Math.min(3, Math.max(0, parseInt(slider.value))); // Ensure value stays within 0-3
            preferenceLabel.textContent = levels[slider.value];
            preferenceLabel.style.color = colors[slider.value];
            localStorage.setItem(slider.dataset.key, slider.value); // Save immediately
            updateProgress(); // Update progress indicator
        }

        function updateProgress() {
            const meSliders = document.querySelectorAll('#me .slider');
            const partnerSliders = document.querySelectorAll('#partner .slider');
            
            const meTotal = meSliders.length;
            const meAdjusted = Array.from(meSliders).filter(slider => parseInt(slider.value) !== 0).length;
            const mePercentage = meTotal > 0 ? Math.round((meAdjusted / meTotal) * 100) : 0;
            document.getElementById('meProgressButton').style.left = `calc(${mePercentage}% - 7px)`; // Center button
            document.getElementById('meProgressText').textContent = `${mePercentage}%`;

            const partnerTotal = partnerSliders.length;
            const partnerAdjusted = Array.from(partnerSliders).filter(slider => parseInt(slider.value) !== 0).length;
            const partnerPercentage = partnerTotal > 0 ? Math.round((partnerAdjusted / partnerTotal) * 100) : 0;
            document.getElementById('partnerProgressButton').style.left = `calc(${partnerPercentage}% - 7px)`; // Center button
            document.getElementById('partnerProgressText').textContent = `${partnerPercentage}%`;
        }

        function generateShareURL() {
            const sliders = document.querySelectorAll('.slider');
            const params = new URLSearchParams();
            sliders.forEach(slider => {
                params.set(slider.dataset.key, slider.value);
            });
            const notes = document.getElementById('personalNotes').value;
            if (notes) params.set('notes', encodeURIComponent(notes));
            return `${window.location.origin}${window.location.pathname}?${params.toString()}`;
        }

        function loadFromURL() {
            const urlParams = new URLSearchParams(window.location.search);
            const sliders = document.querySelectorAll('.slider');
            let hasParams = false;
            const colors = ["#FF4D4D", "#FF914D", "#FFD700", "#00CC66"];
            sliders.forEach(slider => {
                const value = urlParams.get(slider.dataset.key);
                if (value !== null) {
                    slider.value = Math.min(3, Math.max(0, parseInt(value))); // Validate value
                    slider.disabled = true;
                    slider.nextElementSibling.textContent = ["Hard Limit", "Curious", "Moderate", "Oh Yes"][slider.value];
                    slider.nextElementSibling.style.color = colors[slider.value];
                    hasParams = true;
                } else if (localStorage.getItem(slider.dataset.key)) {
                    slider.value = localStorage.getItem(slider.dataset.key);
                    slider.nextElementSibling.textContent = ["Hard Limit", "Curious", "Moderate", "Oh Yes"][slider.value];
                    slider.nextElementSibling.style.color = colors[slider.value];
                }
            });
            const notes = urlParams.get('notes');
            if (notes) {
                document.getElementById('personalNotes').value = decodeURIComponent(notes);
                document.getElementById('personalNotes').disabled = true;
            } else if (localStorage.getItem('personalNotes')) {
                document.getElementById('personalNotes').value = localStorage.getItem('personalNotes');
            }
            updateProgress(); // Update progress on load
            return hasParams;
        }

        function toggleEditMode() {
            const sliders = document.querySelectorAll('.slider');
            const notes = document.getElementById('personalNotes');
            sliders.forEach(slider => slider.disabled = !slider.disabled);
            notes.disabled = !notes.disabled;
            document.querySelector('.edit-button').textContent = sliders[0].disabled ? 'Edit' : 'Lock';
        }

        function resetSliders() {
            const sliders = document.querySelectorAll('.slider');
            sliders.forEach(slider => {
                slider.value = 0;
                slider.disabled = false;
                updatePreference(slider, slider.nextElementSibling);
                localStorage.removeItem(slider.dataset.key); // Remove only slider-specific storage
            });
            document.querySelector('.edit-button').textContent = 'Edit';
            document.getElementById('saveMessage').style.display = 'block';
            document.getElementById('saveMessage').textContent = 'Sliders Reset!';
            setTimeout(() => document.getElementById('saveMessage').style.display = 'none', 1000);
            updateProgress(); // Update progress after reset
        }

        function shareToTelegram() {
            const url = generateShareURL();
            window.open(`https://t.me/share/url?url=${encodeURIComponent(url)}`, '_blank');
        }

        function shareToWhatsApp() {
            const url = generateShareURL();
            window.open(`https://wa.me/?text=${encodeURIComponent(url)}`, '_blank');
        }

        function shareToEmail() {
            const url = generateShareURL();
            const subject = encodeURIComponent("My Kinky Play List");
            const body = encodeURIComponent(`Check out my preferences: ${url}`);
            window.open(`mailto:?subject=${subject}&body=${body}`, '_blank');
        }

        function openTab(tabName) {
            const tabs = document.querySelectorAll('.tab-content');
            const buttons = document.querySelectorAll('.tab-button');
            tabs.forEach(tab => tab.classList.remove('active'));
            buttons.forEach(button => button.classList.remove('active'));
            document.getElementById(tabName).classList.add('active');
            document.querySelector(`button[onclick="openTab('${tabName}')"]`).classList.add('active');
        }

        document.addEventListener('DOMContentLoaded', () => {
            const isShared = loadFromURL();
            const notesTextArea = document.getElementById('personalNotes');
            const saveMessage = document.getElementById('saveMessage');
            
            // Auto-save notes on input
            notesTextArea.addEventListener('input', () => {
                localStorage.setItem('personalNotes', notesTextArea.value);
                saveMessage.style.display = 'block';
                saveMessage.textContent = 'Saved!';
                setTimeout(() => saveMessage.style.display = 'none', 1000);
            });

            if (isShared) {
                document.querySelector('.edit-button').style.display = 'block';
            }
        });
    </script>
</body>
</html>
